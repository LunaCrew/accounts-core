{
  "name": "luna-app-backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.ts",
  "_moduleAliases": {
    "src": "./build/src/"
  },
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register src/app.ts",
    "debug": "npm run build && npm run dev",
    "start": "node build/src/app.js",
    "test": "npm run test:coverage",
    "test:unit": "jest --verbose ./test/unit/",
    "test:coverage": "jest --verbose ./test/unit/ --coverage",
    "build": "tsc",
    "lint": "npx eslint . --fix",
    "reset": "rm -rf node_modules && rm package-lock.json && npm install",
    "clear:build": "rm -rf build",
    "clear:coverage": "rm -rf coverage",
    "prepare": "husky install",
    "help": "bash scripts/scripts-list.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LunaCrew/luna-app-backend.git"
  },
  "author": "Ash Trindade",
  "license": "All rights reserved",
  "bugs": {
    "url": "https://github.com/LunaCrew/luna-app-backend/issues"
  },
  "homepage": "https://github.com/LunaCrew/luna-app-backend#readme",
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/jest": "29.5.11",
    "@types/module-alias": "2.0.4",
    "@types/node": "20.10.4",
    "@types/swagger-ui-express": "4.1.6",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "eslint": "8.55.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "nodemon": "3.0.2",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@ashtrindade/logger": "2.0.0",
    "dotenv": "16.3.1",
    "express": "4.19.2",
    "joi": "17.11.0",
    "mongodb": "6.3.0",
    "swagger-ui-express": "5.0.0",
    "uuidv4": "6.2.13"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": ".ts, .js",
    "ignore": []
  }
}
